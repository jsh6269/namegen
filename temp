/*
 * Sample a random index according to the given probability distribution
 * This function is called at most N*MAX_LEN times. Each call uses a
 * random float in [0,1] to sample an index from the given distribution.
 * input: [NUM_CHAR], probability distribution of the characters
 * rng_seq: [N*MAX_LEN],
 */

// sum input[i] for 0 <= i <n
// if sum > r 
int random_select(float *input, float *rng_seq, int rng_offset, size_t n) {
  float r = rng_seq[rng_offset];
  float psum = 0.0;
  for (size_t i = 0; i < n; i++) {
    psum += input[i];
    if (psum > r) {
      return i;
    }
  }
  return n - 1;
}

/*
 * Initialize the model.
 * Do input-independent job here.
 */
void namegen_initialize(int N, char *parameter_fname) {

  /* Only the root process reads the parameter */
 
  size_t parameter_binary_size = 0;
  float *parameter =
      (float *)read_binary(parameter_fname, &parameter_binary_size);

  /* Network parameters */
  character_embedding =
      new Tensor({NUM_CHAR, EMBEDDING_DIM}, parameter + OFFSET0);

  W_ir0 = new Tensor({HIDDEN_DIM, EMBEDDING_DIM}, parameter + OFFSET1);
  W_iz0 = new Tensor({HIDDEN_DIM, EMBEDDING_DIM}, parameter + OFFSET2);
  W_in0 = new Tensor({HIDDEN_DIM, EMBEDDING_DIM}, parameter + OFFSET3);
  W_ir1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET4);
  W_iz1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET5);
  W_in1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET6);

  W_hr0 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET7);
  W_hz0 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET8);
  W_hn0 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET9);
  W_hr1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET10);
  W_hz1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET11);
  W_hn1 = new Tensor({HIDDEN_DIM, HIDDEN_DIM}, parameter + OFFSET12);

  b_ir0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET13);
  b_iz0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET14);
  b_in0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET15);
  b_ir1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET16);
  b_iz1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET17);
  b_in1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET18);

  b_hr0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET19);
  b_hz0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET20);
  b_hn0 = new Tensor({HIDDEN_DIM}, parameter + OFFSET21);
  b_hr1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET22);
  b_hz1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET23);
  b_hn1 = new Tensor({HIDDEN_DIM}, parameter + OFFSET24);

  W_fc = new Tensor({NUM_CHAR, HIDDEN_DIM}, parameter + OFFSET25);
  b_fc = new Tensor({NUM_CHAR}, parameter + OFFSET26);

  /* input, activations, output, etc. */
  input = new Tensor({1, N});
  emb_out = new Tensor({EMBEDDING_DIM, N});

  hidden0 = new Tensor({HIDDEN_DIM, N});
  hidden1 = new Tensor({HIDDEN_DIM, N});

  r0 = new Tensor({HIDDEN_DIM, N});
  r1 = new Tensor({HIDDEN_DIM, N});
  z0 = new Tensor({HIDDEN_DIM, N});
  z1 = new Tensor({HIDDEN_DIM, N});
  n0 = new Tensor({HIDDEN_DIM, N});
  n1 = new Tensor({HIDDEN_DIM, N});
  f = new Tensor({NUM_CHAR, N});

  rtmp00 = new Tensor({HIDDEN_DIM, N});
  rtmp01 = new Tensor({HIDDEN_DIM, N});
  rtmp02 = new Tensor({HIDDEN_DIM, N});
  rtmp03 = new Tensor({HIDDEN_DIM, N});
  rtmp04 = new Tensor({HIDDEN_DIM, N});
  rtmp10 = new Tensor({HIDDEN_DIM, N});
  rtmp11 = new Tensor({HIDDEN_DIM, N});
  rtmp12 = new Tensor({HIDDEN_DIM, N});
  rtmp13 = new Tensor({HIDDEN_DIM, N});
  rtmp14 = new Tensor({HIDDEN_DIM, N});

  ztmp00 = new Tensor({HIDDEN_DIM, N});
  ztmp01 = new Tensor({HIDDEN_DIM, N});
  ztmp02 = new Tensor({HIDDEN_DIM, N});
  ztmp03 = new Tensor({HIDDEN_DIM, N});
  ztmp04 = new Tensor({HIDDEN_DIM, N});
  ztmp10 = new Tensor({HIDDEN_DIM, N});
  ztmp11 = new Tensor({HIDDEN_DIM, N});
  ztmp12 = new Tensor({HIDDEN_DIM, N});
  ztmp13 = new Tensor({HIDDEN_DIM, N});
  ztmp14 = new Tensor({HIDDEN_DIM, N});

  ntmp00 = new Tensor({HIDDEN_DIM, N});
  ntmp01 = new Tensor({HIDDEN_DIM, N});
  ntmp02 = new Tensor({HIDDEN_DIM, N});
  ntmp03 = new Tensor({HIDDEN_DIM, N});
  ntmp04 = new Tensor({HIDDEN_DIM, N});
  ntmp05 = new Tensor({HIDDEN_DIM, N});
  ntmp10 = new Tensor({HIDDEN_DIM, N});
  ntmp11 = new Tensor({HIDDEN_DIM, N});
  ntmp12 = new Tensor({HIDDEN_DIM, N});
  ntmp13 = new Tensor({HIDDEN_DIM, N});
  ntmp14 = new Tensor({HIDDEN_DIM, N});
  ntmp15 = new Tensor({HIDDEN_DIM, N});

  htmp00 = new Tensor({HIDDEN_DIM, N});
  htmp01 = new Tensor({HIDDEN_DIM, N});
  htmp02 = new Tensor({HIDDEN_DIM, N});
  htmp10 = new Tensor({HIDDEN_DIM, N});
  htmp11 = new Tensor({HIDDEN_DIM, N});
  htmp12 = new Tensor({HIDDEN_DIM, N});

//  rfloats = new Tensor({N * MAX_LEN});
  rfloats = (float *)malloc(sizeof(float) * N * MAX_LEN);
  ftmp0 = new Tensor({NUM_CHAR, N});
  char_prob = new Tensor({NUM_CHAR, N});

  CHECK_CUDA(cudaMalloc(&exps, sizeof(float) * NUM_CHAR * N));
  CHECK_CUDA(cudaMalloc(&sm_output_gpu, ((NUM_CHAR + 2048 - 1) / 2048) * sizeof(float)));

}

/*
 * Generate names.
 * Any input-dependent computation/communication must be done here.
 * N: # of names to generate
 * random_floats: N*MAX_LEN sequence of random floats in [0,1].
 * output: 2D-array of size N x (MAX_LEN+1), allocaetd at main.cpp
 */

__global__ void gpu_set_val(float *buf, int N, float val) {
  int tidx = blockIdx.x * blockDim.x + threadIdx.x;
  if(tidx >= N){
    return;
  }
  buf[tidx] = val;
}

void namegen(int N, float *random_floats, char *output) {

  memcpy(rfloats, random_floats, N * MAX_LEN * sizeof(float));
  memset(output, 0, N * (MAX_LEN + 1) * sizeof(char));

    /* Generate N names */
    /* Initialize input and hidden vector. */
    /* One hidden vector for each GRU layer */
    gpu_set_val<<<(N+512-1)/512, 512>>>(input->buf, 1, SOS);

    dim3 gridDim1((hidden0->num_elem() + 512 - 1) / 512);
    dim3 blockDim1(512);
    gpu_set_val<<<gridDim1, blockDim1>>>(hidden0->buf, hidden0->num_elem(), 0);
    dim3 gridDim2((hidden1->num_elem() + 512 - 1) / 512);
    dim3 blockDim2(512);
    gpu_set_val<<<gridDim2, blockDim2>>>(hidden1->buf, hidden1->num_elem(), 0);

    for (int l = 0; l < MAX_LEN; l++) {
      /* Embedding */
      embedding(input, character_embedding, emb_out, N);

      /* First layer r */
      matvec(W_ir0, emb_out, rtmp00, N);
      matvec(W_hr0, hidden0, rtmp01, N);
      elemwise_add(rtmp00, b_ir0, rtmp02, N);
      elemwise_add(rtmp02, rtmp01, rtmp03, N);
      elemwise_add(rtmp03, b_hr0, rtmp04, N);
      elemwise_sigmoid(rtmp04, r0, N);

      /* First layer z */
      matvec(W_iz0, emb_out, ztmp00, N);
      matvec(W_hz0, hidden0, ztmp01, N);
      elemwise_add(ztmp00, b_iz0, ztmp02, N);
      elemwise_add(ztmp02, ztmp01, ztmp03, N);
      elemwise_add(ztmp03, b_hz0, ztmp04, N);
      elemwise_sigmoid(ztmp04, z0, N);

      /* First layer n */
      matvec(W_in0, emb_out, ntmp00, N);
      elemwise_add(ntmp00, b_in0, ntmp01, N);
      matvec(W_hn0, hidden0, ntmp02, N);
      elemwise_add(ntmp02, b_hn0, ntmp03, N);
      elemwise_mul(r0, ntmp03, ntmp04, N);
      elemwise_add(ntmp01, ntmp04, ntmp05, N);
      elemwise_tanh(ntmp05, n0, N);

      /* First layer h (hidden) */
      elemwise_oneminus(z0, htmp00, N);
      elemwise_mul(htmp00, n0, htmp01, N);
      elemwise_mul(z0, hidden0, htmp02, N);
      elemwise_add(htmp01, htmp02, hidden0, N);

      /* Second layer r */
      matvec(W_ir1, hidden0, rtmp10, N);
      matvec(W_hr1, hidden1, rtmp11, N);
      elemwise_add(rtmp10, b_ir1, rtmp12, N);
      elemwise_add(rtmp12, rtmp11, rtmp13, N);
      elemwise_add(rtmp13, b_hr1, rtmp14, N);
      elemwise_sigmoid(rtmp14, r1, N);

      /* Second layer z */
      matvec(W_iz1, hidden0, ztmp10, N);
      matvec(W_hz1, hidden1, ztmp11, N);
      elemwise_add(ztmp10, b_iz1, ztmp12, N);
      elemwise_add(ztmp12, ztmp11, ztmp13, N);
      elemwise_add(ztmp13, b_hz1, ztmp14, N);
      elemwise_sigmoid(ztmp14, z1, N);

      /* Second layer n */ // error?
      matvec(W_in1, hidden0, ntmp10, N);
      elemwise_add(ntmp10, b_in1, ntmp11, N);
      matvec(W_hn1, hidden1, ntmp12, N); // error?
      elemwise_add(ntmp12, b_hn1, ntmp13, N);
      elemwise_mul(r1, ntmp13, ntmp14, N);
      elemwise_add(ntmp11, ntmp14, ntmp15, N);
      elemwise_tanh(ntmp15, n1, N);

      /* Second layer h (hidden) */
      elemwise_oneminus(z1, htmp10, N);
      elemwise_mul(htmp10, n1, htmp11, N);
      elemwise_mul(z1, hidden1, htmp12, N);
      elemwise_add(htmp11, htmp12, hidden1, N);

      /* Fully connected layer */
      matvec(W_fc, hidden1, ftmp0, N);
      elemwise_add(ftmp0, b_fc, f, N);
      /* Softmax */
      softmax(f, char_prob, N);
      
      /* Random select */
      // float* char_prob_cpu;
      // size_t lenCharProb = char_prob->num_elem() / N;
      // CHECK_CUDA(cudaMallocHost(&char_prob_cpu, sizeof(float) * lenCharProb));
      // CHECK_CUDA(cudaMemcpy(char_prob_cpu, char_prob->buf, sizeof(float) * lenCharProb, cudaMemcpyDeviceToHost));
      // for(int n=0; n<N; n++){
      //   int selected_char = random_select(char_prob_cpu + (n*lenCharProb), rfloats, n * MAX_LEN + l, lenCharProb);        
      //   gpu_set_val<<<1, 1>>>(input->buf, 1, selected_char);
      //   output[n * (MAX_LEN + 1) + l] = selected_char;
      // }
    }
}

/*
 * Finalize the model.
 * Although it is not neccessary, we recommend to deallocate and destruct
 * everything you made in namegen_initalize() and namegen().
 */
